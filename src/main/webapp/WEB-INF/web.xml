<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>Intranet</servlet-name>
  	<display-name>Intranet</display-name>
  	<description></description>
  	<servlet-class>com.qrassistance.controller.IntranetServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Assistance</servlet-name>
  	<display-name>Assistance</display-name>
  	<description></description>
  	<servlet-class>com.qrassistance.controller.AssistanceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>Intranet</servlet-name>
  	<url-pattern>/Intranet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Assistance</servlet-name>
  	<url-pattern>/Assistance</url-pattern>
  </servlet-mapping>
</web-app>
